 <table>
    <thead>
        <tr>
            <th> Patients ID</th>
            <th> First Name</th>
            <th> Last Name</th>
            <th> Date Of Birth</th>
        </tr>
    </thead>
    <tbody>
        <% data.forEach(row =>{%>
            <tr>
                <td> <%= row.patient_id %></td>
                <td> <%= row.first_name %></td>
                <td> <%= row.last_name %></td>
                <td> <%= row.date_of_birth %></td>
            </tr>
        <%
        });%>
    </tbody>
 </table> 

